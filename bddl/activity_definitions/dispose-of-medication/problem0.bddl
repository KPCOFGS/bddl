(define (problem dispose-of-medication_0)
    (:domain omnigibson)

    (:objects
        aspirin.n.01_1 aspirin.n.01_2 aspirin.n.01_3 aspirin.n.01_4 - aspirin.n.01
        pill.n.02_1 - pill.n.02
        pill_bottle.n.01_1 pill_bottle.n.01_2 - pill_bottle.n.01
        shelf.n.01_1 - shelf.n.01
        bin.n.01_1 - bin.n.01
        floor.n.01_1 - floor.n.01
