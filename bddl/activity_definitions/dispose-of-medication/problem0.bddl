        (inside aspirin.n.01_2 pill_bottle.n.01_1) 
        (inside aspirin.n.01_3 pill_bottle.n.01_1) 
        (inside aspirin.n.01_4 pill_bottle.n.01_1) 
        (filled pill_bottle.n.01_2 pill.n.02_1)
