    (:init 
        (inside die.n.01_1 cabinet.n.01_1) 
        (inside board_game.n.01_1 cabinet.n.01_1) 
        (inside puzzle.n.02_1 cabinet.n.01_1) 
        (inroom floor.n.01_1 childs_room) 
