     	die.n.01_1 - die.n.01
    	cabinet.n.01_1 - cabinet.n.01
    	board_game.n.01_1 - board_game.n.01
    	puzzle.n.02_1 - puzzle.n.02
    	floor.n.01_1 - floor.n.01
    	table.n.02_1 - table.n.02
    	agent.n.01_1 - agent.n.01
