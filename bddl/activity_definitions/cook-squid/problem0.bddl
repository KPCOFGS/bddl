            (nextto ?plate.n.04_1 ?stove.n.01_1) 
            (ontop ?squid.n.01_1 ?plate.n.04_1) 
            (covered ?frying_pan.n.01_1 ?olive_oil.n.01_1) 
            (covered ?squid.n.01_1 ?salt.n.02_1) 
            (ontop ?frying_pan.n.01_1 ?stove.n.01_1)
            (nextto ?salt__shaker.n.01_1 ?stove.n.01_1) 
            (cooked ?squid.n.01_1) 
            (hot ?frying_pan.n.01_1) 
            (not 
