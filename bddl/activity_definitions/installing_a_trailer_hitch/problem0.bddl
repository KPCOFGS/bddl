(define (problem installing_a_trailer_hitch-0)
    (:domain omnigibson)

    (:objects
        trailer_truck.n.01_1 - trailer_truck.n.01
        floor.n.01_1 - floor.n.01
        hitch.n.04_1 - hitch.n.04
        agent.n.01_1 - agent.n.01
    )
    
    (:init 
        (ontop trailer_truck.n.01_1 floor.n.01_1) 
        (ontop hitch.n.04_1 floor.n.01_1) 
        (not 
            (attached hitch.n.04_1 trailer_truck.n.01_1)
        )
        (inroom floor.n.01_1 garage) 
        (ontop agent.n.01_1 floor.n.01_1)
    )
    
    (:goal 
        (and 
            (attached ?hitch.n.04_1 ?trailer_truck.n.01_1)
        )
    )
)